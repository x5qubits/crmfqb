{
    "name": "itrack/anaf",
    "type": "library",
    "description": "API ANAF pentru verificarea contribuabililor care sunt inregistrati conform art. 316 din Codul Fiscal",
    "license": "MIT",
    "homepage": "https://devgeeks.ro",
    "authors": [
        {
            "name": "Alex Stoica",
            "email": "0x01.alex+github@gmail.com"
        }
    ],
    "keywords": [
        "api anaf",
        "verificare tva",
        "inregistrat tva",
        "webservie anaf",
        "date firma",
        "mfinante"
    ],
    "require": {
        "php": ">=7.1 || >=8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.20 || ^9.5.2"
    },
    "autoload": {
        "psr-4": {
            "Itrack\\Anaf\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Itrack\\Anaf\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
